{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "dueDate": {
      "type": "datetime",
      "required": true
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "completed": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  }
}
